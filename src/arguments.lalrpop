use crate::parser::Node;

grammar;


Node: Node = {
    <n:r"[[A-Za-z0-9]]+"> => Node::StrLiteral(n.to_string()),
    <n:r#""\w+""#> => Node::StrLiteral(n.to_string()),
};

pub Args: Node = <mut v:(<Node> ",")*> <e:Node?> => Node::Array(match e {
        None => v,
        Some(e) => {
            v.push(e);
            v
        }
    }); 


